der.productId === product.id) {\n                        td = (\n                            <td key={i}>\n                                {product[header.field]}\n                            </td>\n                        )\n                    }\n                })\n            } else (\n                td = (\n                    <td key={i}>\n                        {value}\n                    </td>\n                )\n            )\n\n        tdArr.push(td)\n        })\n        const tr = (\n            <tr key={i}>\n                {tdArr}\n            </tr>\n        )\n        trArr.push(tr)\n    })\n    \n    return (\n        <div className=\"table-responsive\">\n            <table className=\"table table-striped\">\n                <thead>\n                    <tr>\n                        {orderTableHeaders.map((header, i) => (\n                            <th key={i}>{header.label || header.field}</th>\n                        ))}\n                    </tr>\n                </thead>\n                <tbody>{trArr}</tbody>\n            </table>\n        </div>\n    )\n}\n","/Users/roman/PhpstormProjects/training/crm-2/src/HttpClient.jsx",[],"/Users/roman/PhpstormProjects/training/crm-2/src/data.js",[],"/Users/roman/PhpstormProjects/training/crm-3/src/index.js",[],["178","179"],"/Users/roman/PhpstormProjects/training/crm-3/src/reportWebVitals.js",[],"/Users/roman/PhpstormProjects/training/crm-3/src/App.js",[],"/Users/roman/PhpstormProjects/training/crm-3/src/pages/NewOrderPage.jsx",[],"/Users/roman/PhpstormProjects/training/crm-3/src/pages/OrderPage.jsx",[],"/Users/roman/PhpstormProjects/training/crm-3/src/pages/ManagerPage/index.jsx",["180","181","182"],"import React from \"react\";\nimport OrderFilter from \"./OrderFilter/OrderFilter\";\nimport Orders from \"./Orders/Orders\";\nimport { Route, NavLink, Switch } from \"react-router-dom\";\nimport Pagination from \"./Pagination/Pagination\";\n\nexport default function Table(props) {\n\n  const {orders, se